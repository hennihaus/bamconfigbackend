  "paths": {},
  "components": {
    "schemas": {
      "Team": {
        "type": "object",
        "required": [
          "uuid",
          "username",
          "password",
          "jmsQueue",
          "students",
          "statistics",
          "hasPassed"
        ],
        "properties": {
          "uuid": {
            "type": "string",
            "format": "uuid",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
          },
          "username": {
            "type": "string",
            "minLength": 6,
            "maxLength": 50
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 10,
            "maxLength": 50
          },
          "jmsQueue": {
            "type": "string",
            "minLength": 6,
            "maxLength": 50
          },
          "students": {
            "type": "array",
            "title": "Students",
            "items": {
              "$ref": "#/components/schemas/Student"
            },
            "minItems": 0,
            "uniqueItems": true
          },
          "statistics": {
            "type": "object",
            "additionalProperties": {
              "type": "integer",
              "format": "int64",
              "minimum": 0
            }
          },
          "hasPassed": {
            "type": "boolean"
          }
        },
        "example": {
