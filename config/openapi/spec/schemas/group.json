            "format": "password",
            "minLength": 8,
            "maxLength": 50
          },
          "jmsQueue": {
            "type": "string",
            "minLength": 6,
            "maxLength": 50
          },
          "students": {
            "type": "array",
            "title": "Students",
            "items": {
              "type": "string"
            },
            "minItems": 0,
            "uniqueItems": true
          },
          "stats": {
            "type": "object",
            "additionalProperties": {
              "type": "integer",
              "format": "int32",
              "minimum": 0
            }
          },
          "hasPassed": {
            "type": "boolean"
          }
        },
