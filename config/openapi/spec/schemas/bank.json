  "paths": {},
  "components": {
    "schemas": {
      "Bank": {
        "type": "object",
        "required": [
          "uuid",
          "name",
          "jmsQueue",
          "thumbnailUrl",
          "isAsync",
          "isActive",
          "teams"
        ],
        "properties": {
          "uuid": {
            "type": "string",
            "format": "uuid",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
          },
          "name": {
            "type": "string",
            "minLength": 6,
            "maxLength": 50
          },
          "jmsQueue": {
            "type": "string",
            "minLength": 6,
            "maxLength": 50
          },
          "thumbnailUrl": {
            "type": "string",
            "format": "url",
            "pattern": "(https?:\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9]+\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]+\\.[^\\s]{2,})"
          },
          "isAsync": {
            "type": "boolean"
          },
          "isActive": {
            "type": "boolean"
          },
          "creditConfiguration": {
            "$ref": "#/components/schemas/CreditConfiguration"
          },
          "teams": {
            "type": "array",
            "title": "Teams",
            "items": {
              "$ref": "./team.json#/components/schemas/Team"
            },
            "minItems": 0,
            "uniqueItems": true
          }
        }
      },
      "CreditConfiguration": {
        "type": "object",
        "required": [
          "minAmountInEuros",
          "maxAmountInEuros",
          "minTermInMonths",
          "maxTermInMonths",
          "minSchufaRating",
          "maxSchufaRating"
        ],
        "properties": {
          "minAmountInEuros": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "maxAmountInEuros": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "minTermInMonths": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "maxTermInMonths": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "minSchufaRating": {
            "$ref": "#/components/schemas/RatingLevel"
          },
          "maxSchufaRating": {
            "$ref": "#/components/schemas/RatingLevel"
          }
        }
      },
      "RatingLevel": {
        "type": "string",
        "enum": [
          "A",
          "B",
          "C",
          "D",
          "E",
          "F",
          "G",
          "H",
          "I",
          "J",
          "K",
          "L",
          "N",
          "O",
          "P"
        ]
      }
    },
